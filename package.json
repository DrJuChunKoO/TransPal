{
  "name": "transpal-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "build:analyze": "astro build --verbose",
    "build:production": "NODE_ENV=production astro build --config astro.config.production.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:markdown": "node scripts/test-markdown.mjs",
    "clean": "rm -rf dist .astro node_modules/.astro",
    "optimize": "npm run clean && npm run build:production",
    "build:monitor": "node scripts/build-monitor.mjs",
    "build:analyze-bundle": "npm run build && node scripts/optimize-build.mjs"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.3.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.11.1",
    "dompurify": "^3.2.6",
    "marked": "^16.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/jsdom": "^21.1.7",
    "@types/marked": "^6.0.0",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "vitest": "^3.2.4"
  }
}